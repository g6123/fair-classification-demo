{
  "extends": ["eslint:recommended", "plugin:import/errors", "plugin:import/warnings", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "env": {
    "browser": false,
    "node": true,
    "es6": true
  },
  "rules": {
    "no-unused-vars": ["warn", { "argsIgnorePattern": "^_" }],
    "no-console": "warn",
    "import/order": ["warn", { "newlines-between": "never" }],
    "prettier/prettier": "warn"
  }
}
